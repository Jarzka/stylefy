;; shadow-cljs configuration
{:lein     false

 :source-paths
           ["src/cljs"

            "../../src/clj"
            "../../src/cljs"
            "../../src/cljc"

            "../../modules/stylefy-reagent/src/cljs"]

 :dependencies
           [[prismatic/dommy "1.1.0"]
            [garden "1.3.10"]
            [reagent "1.2.0"]
            [com.andrewmcveigh/cljs-time "0.5.0"]]

 :dev-http {8000 "resources/public"}

 :builds
           {:frontend
            {:target     :browser
             :output-dir "resources/public/js/"
             :devtools   {:after-load stylefy.examples.reagent.main/start}
             :modules    {:main {:init-fn stylefy.examples.reagent.main/start}}}}}
